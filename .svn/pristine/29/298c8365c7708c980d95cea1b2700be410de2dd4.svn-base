// 发起合同修订
function startContractReversionAudit(options){
  var data = options.data;
  var data1 = options.data1;
  var remark = options.remark;
  var callback = options.callback; 

  var conf = {
  	"form_id":24,
    "业务对象":"fp_field_businessObj", 
    "业务对象1":"fp_field_businessObj_1", 
    "修订内容":"fp_field_html", 
    //合同信息
    "合同Id":"fp_field_contractId",
    "合同编号":"fp_field_contractNo", 
    "客户":"fp_field_customerName",
    "客户编号":"fp_field_customerId",
    "含加工工序":"fp_field_hasProcedure",
    "是否外销":"fp_field_isForeign",
    "业务员":"fp_field_salesName",
    "业务员编号":"fp_field_salesId",
    "签订日期":"fp_field_signAddress",
    "签订地点":"fp_field_signAddress",
    "合同金额":"fp_field_amount",
    "币种":"fp_field_currencyName",
    "交货地点":"fp_field_deliveryAddress",
    "备注":"fp_field_remark",
    "修订原因":"fp_field_reason", 
  };
  var fd = {
  	processKey:conf.form_id
  };
  fd.businessKey = data.contractId;

  fd[conf["合同Id"]] = data.contractId;
  fd[conf["合同编号"]] = data.contractNo;
  fd[conf["客户"]] = data.customerName;
  fd[conf["客户编辑"]] = data.customerName;
  fd[conf["业务员"]] = data.salesName;
  fd[conf["业务员编号"]] = data.sales;
  fd[conf["签订日期"]] = data.signDate;
  fd[conf["签订地点"]] = data.signAddress;
  fd[conf["合同金额"]] = data.amount;
  fd[conf["币种"]] = data.currencyName;
  fd[conf["交货地点"]] = data.deliveryAddress;
  fd[conf["备注"]] = data.remark;

  fd[conf["业务对象"]] = JSON.stringify(data);
  fd[conf["业务对象1"]] = JSON.stringify(data1); 

  fd[conf["修订原因"]] = remark;

  fd[conf["修订内容"]] = ''
  + '<h3>表头</h3>'
  + getBillHtml(data,data1)
  + '<h3>明细</h3>' 
  + getDetailHtml(data,data1)
  + '<h3/>修订原因</h3>'
  + remark;

  console.log(fd[conf["修订内容"]]);

  utils.start(fd,(err,dt)=>{
  	if(err){
  		callback(err);
  	}else{
  		callback(null,dt.procInstId);
  	}
  });
}

function getBillHtml(data,data1){
  var bill = 
  utils.buildTableHtml([
    {
      title:'字段',
      key:'field'
    },
    {
      title:'修改前',
      key:'old_val'
    },
    {
      title:'修改后',
      key:'new_val',
      render:(index,row)=>{
        if(row.old_val != row.new_val){
          return '<span style="color:red">'+ row.new_val +'</span>';
        }
        return '';
      }
    }
  ],[
    {field:'合同编号',old_val:data.contractNo,new_val:data1.contractNo},
    {field:'客户',old_val:data.customerName,new_val:data1.customerName},
    {field:'含加工工序',old_val:data.hasProcedure,new_val:data1.hasProcedure},
    {field:'是否外销',old_val:data.isForeign,new_val:data1.isForeign},
    {field:'业务员',old_val:data.salesName,new_val:data1.salesName},
    {field:'签订日期',old_val:data.signDate,new_val:data1.signDate},
    {field:'签订地点',old_val:data.signAddress,new_val:data1.signAddress},
    {field:'合同金额',old_val:data.amount,new_val:data1.amount},
    {field:'币种',old_val:data.currencyName,new_val:data1.currencyName},
    {field:'交货地点',old_val:data.deliveryAddress,new_val:data1.deliveryAddress},
    {field:'备注',old_val:data.remark,new_val:data1.remark}
  ]);
  return bill;
}

function getDetailHtml(data,data1){
  var pairs = [];
  var news = [];
  var newsIndex = {};
  for(var i = 0;i<data1.detailList.length;i++){
    news.push(data1.detailList[i]);
    newsIndex[data1.detailList[i].id] = data1.detailList[i];
  }
  // 比对修改的
  for(var i = 0;i<data.detailList.length;i++){
    var pair = {
      old:data.detailList[i],
      new:null
    }
    if(newsIndex[pair.old.id]){
      pair.new = newsIndex[pair.old.id];
      news.splice(news.indexOf(pair.new),1);
    }
    pairs.push(pair);
  }

  // 比对新增的
  for(var i = 0;i<news.length;i++){
    var pair = {
      old:null,
      new:news[i]
    }
    pairs.push(pair);
  } 

  function render(pair,field){
    var html = '';

    if(pair.old){
      html += utils.encode(pair.old[field]);
    }
    if(pair.new){
      if(pair.old == null || pair.old[field] != pair.new[field]){
        html += '<span style="color:red">( '+ utils.encode(pair.new[field]) +' )</span>';
      }
    }
    return html;
  }

  return utils.buildTableHtml([
    {
      title:'序号',
      render:(index,row)=>{
