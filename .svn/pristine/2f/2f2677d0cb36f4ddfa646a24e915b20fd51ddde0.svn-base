package com.lyarc.tp.corp.workflow.bean;

import java.time.LocalDateTime;
import java.util.List;

import com.lyarc.tp.corp.contacts.bean.EmployeeBean;

/**
 * 
 * 报表
 * 
 * @version 1.0
 * @since JDK1.8
 * @author jianghan
