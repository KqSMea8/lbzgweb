public class WorkflowReport {
    /**
     * 编号
     */
    private Integer id;
    /**
     * 表单ID
     */
    private Integer formId;
    /**
     * 标题
     */
    private String title;
    /**
     * 类型 1.查询 2.统计表 3.统计图
     */
    private Integer type;
    /**
     * 授权人员
     */
    private String authEmpIds;
    
    private List<EmployeeBean> empList;
    /**
     * 穿透 1.是
     */
    private Integer rift;
    /**
     * 筛选规则
     */
    private String filterJson;
    /**
     * 数据项，逗号隔开
     */
    private String datas;
    /**
     * 查询项，逗号隔开
     */
    private String querys;
    /**
     * 分组项，逗号隔开
     */
    private String groups;
    /**
     * 交叉列项，逗号隔开
     */
    private String crosses;
    /**
     * 统计项
     */
    private String summationJson;
    /**
     * 行汇总
     */
    private String rowSummationJson;
    /**
     * 图表 1.饼 2.条 3.柱 4.拆 5.面
     */
    private Integer chart;
    /**
     * 可以切换 1.是
     */
    private Integer allowSwitch;
    /**
     * 创建时间
     */
    private LocalDateTime createTime;
    /**
     * 更新时间 
     */
    private LocalDateTime updateTime;

    private String fieldsJson;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getFormId() {
        return formId;
    }

    public void setFormId(Integer formId) {
        this.formId = formId;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public Integer getType() {
        return type;
    }

    public void setType(Integer type) {
        this.type = type;
    }

    public String getAuthEmpIds() {
        return authEmpIds;
    }

    public void setAuthEmpIds(String authEmpIds) {
        this.authEmpIds = authEmpIds;
    }

    public Integer getRift() {
        return rift;
    }

    public void setRift(Integer rift) {
        this.rift = rift;
    }

    public String getFilterJson() {
        return filterJson;
    }

    public void setFilterJson(String filterJson) {
        this.filterJson = filterJson;
    }

    public String getDatas() {
        return datas;
    }

    public void setDatas(String datas) {
        this.datas = datas;
    }

    public String getQuerys() {
        return querys;
    }

    public void setQuerys(String querys) {
        this.querys = querys;
    }

    public String getGroups() {
        return groups;
    }

    public void setGroups(String groups) {
        this.groups = groups;
    }

    public String getCrosses() {
        return crosses;
    }

    public void setCrosses(String crosses) {
        this.crosses = crosses;
    }

    public String getSummationJson() {
        return summationJson;
    }

    public void setSummationJson(String summationJson) {
        this.summationJson = summationJson;
    }

    public String getRowSummationJson() {
        return rowSummationJson;
    }

    public void setRowSummationJson(String rowSummationJson) {
        this.rowSummationJson = rowSummationJson;
    }

    public Integer getChart() {
        return chart;
    }

    public void setChart(Integer chart) {
        this.chart = chart;
    }

    public Integer getAllowSwitch() {
        return allowSwitch;
    }

    public void setAllowSwitch(Integer allowSwitch) {
        this.allowSwitch = allowSwitch;
    }

    public LocalDateTime getCreateTime() {
        return createTime;
    }

    public void setCreateTime(LocalDateTime createTime) {
        this.createTime = createTime;
    }

    public LocalDateTime getUpdateTime() {
        return updateTime;
    }

    public void setUpdateTime(LocalDateTime updateTime) {
        this.updateTime = updateTime;
    }

    public String getFieldsJson() {
        return fieldsJson;
    }

    public void setFieldsJson(String fieldsJson) {
        this.fieldsJson = fieldsJson;
    }

    public List<EmployeeBean> getEmpList() {
        return empList;
    }

    public void setEmpList(List<EmployeeBean> empList) {
        this.empList = empList;
    }
    
    
}
