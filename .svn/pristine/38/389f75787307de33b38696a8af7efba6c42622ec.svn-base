package com.lyarc.tp.corp.login.form;


import com.lyarc.tp.corp.contacts.bean.EmployeeBean;
import com.lyarc.tp.corp.login.bean.UserInfo;

import java.util.List;

/**
 * 
 * 封装session 保存的用户信息
 * 
 * @version 1.0
 * @since JDK1.8
 * @author zhangjingtao
 * @company 洛阳艾克科技有限公司
 * @copyright (c) 2017 LuoYang ARC Co'Ltd Inc. All rights reserved.
 * @date 2017年8月9日 上午11:54:27
 */
public class LoginUser {
    private String corpId;

    private String corpName;

    private String corpShortName;


    /** 账号表Id */
    private Integer userId;

    /** 登录名 */
    private String loginName;

    private String nickName;

    private String header;

    /** 微信对象信息 */
    private UserInfo userInfo;

    /** 是否是超管 */
    private Integer isSuper;
    
    /** 圆角logo*/
    private String corpRoundLogoUrl;
    
    /** 方形logo*/
    private String corpQuareLogoUrl;
    
    /** 真实姓名*/
    private String trueName;
    
    /** 登录权限（功能点）*/
    private List<String> powerKeys;

    private String empId;

    private EmployeeBean empInfo;

    private String deptName;



    public String getLoginName() {
        return loginName;
    }

    public void setLoginName(String loginName) {
        this.loginName = loginName;
    }

    public UserInfo getUserInfo() {
        return userInfo;
    }

    public void setUserInfo(UserInfo userInfo) {
        this.userInfo = userInfo;
    }

    public Integer getIsSuper() {
        return isSuper;
    }

    public void setIsSuper(Integer isSuper) {
        this.isSuper = isSuper;
    }

    public String getCorpRoundLogoUrl() {
        return corpRoundLogoUrl;
    }

    public void setCorpRoundLogoUrl(String corpRoundLogoUrl) {
        this.corpRoundLogoUrl = corpRoundLogoUrl;
    }

    public String getCorpQuareLogoUrl() {
        return corpQuareLogoUrl;
    }

    public void setCorpQuareLogoUrl(String corpQuareLogoUrl) {
        this.corpQuareLogoUrl = corpQuareLogoUrl;
    }

    public String getTrueName() {
        return trueName;
    }

    public void setTrueName(String trueName) {
        this.trueName = trueName;
    }

    public List<String> getPowerKeys() {
        return powerKeys;
    }

    public void setPowerKeys(List<String> powerKeys) {
        this.powerKeys = powerKeys;
    }

    public String getEmpId() {
        return empId;
    }

    public void setEmpId(String empId) {
        this.empId = empId;
    }

    public Integer getUserId() {
        return userId;
    }

    public void setUserId(Integer userId) {
        this.userId = userId;
    }

    public String getCorpId() {
        return corpId;
    }

    public void setCorpId(String corpId) {
        this.corpId = corpId;
    }

    public String getCorpName() {
        return corpName;
    }

    public void setCorpName(String corpName) {
        this.corpName = corpName;
    }

    public String getCorpShortName() {
        return corpShortName;
    }

    public void setCorpShortName(String corpShortName) {
        this.corpShortName = corpShortName;
    }

    public String getNickName() {
        return nickName;
    }

    public void setNickName(String nickName) {
        this.nickName = nickName;
    }

    public String getHeader() {
        return header;
    }

    public void setHeader(String header) {
        this.header = header;
    }

    public EmployeeBean getEmpInfo() {
        return empInfo;
    }

    public void setEmpInfo(EmployeeBean empInfo) {
        this.empInfo = empInfo;
    }

    public String getDeptName() {
        return deptName;
    }

    public void setDeptName(String deptName) {
        this.deptName = deptName;
    }
}

