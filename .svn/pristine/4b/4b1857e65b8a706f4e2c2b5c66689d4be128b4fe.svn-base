	public void setBatch(String batch) {
		this.batch = batch;
	}

	public LocalDate getMadeDate() {
		return madeDate;
	}

	public void setMadeDate(LocalDate madeDate) {
		this.madeDate = madeDate;
	}

	public LocalDate getValidityDate() {
		return validityDate;
